{
  "name": "accessionator",
  "title": "ENCODE Accessionator",
  "summary": "Accessions DNAnexus files to ENCODE",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "categories": [
    "ENCODE Analysis Pipeline"
  ],
  "inputSpec": [
    {
      "name": "folder_name",
      "class": "string",
      "optional": true,
      "default": "/"
    },
    {
      "name": "key_name",
      "class": "string",
      "optional": true,
      "default": "default"
    },
    {
      "name": "debug",
      "class": "boolean",
      "optional": true,
      "default": false
    }
  ],
  "outputSpec": [
    {
      "name": "file_mapping",
      "optional": true,
      "class": "array:string"
    },
    {
      "name": "outfile",
      "class": "file"
    }
  ],
  "runSpec": {
    "interpreter": "python2.7",
    "file": "src/accessionator.py",
    "execDepends": [
      {"name": "awscli", "package_manager": "pip"}
    ]
  },
  "access": {
    "project": "CONTRIBUTE",
    "network": [
      "*"
    ]
  },
  "authorizedUsers": []
}
